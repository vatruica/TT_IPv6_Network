
# the below configuration is for the Central Europe - Denmark/Copenhagen - time zone
# replace with the settings from your time zone
config system
	option hostname	OpenWrt
	option 'zonename' 'Europe/Copenhagen'
	option 'timezone' 'CET-1CEST-2,M3.5.0/02:00:00,M10.5.0/03:00:00'

#config timeserver ntp
#	list server	0.openwrt.pool.ntp.org
#	list server	1.openwrt.pool.ntp.org
#	list server	2.openwrt.pool.ntp.org
#	list server	3.openwrt.pool.ntp.org
